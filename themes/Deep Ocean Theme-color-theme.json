        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#ff9e60"
      }
    },
    {
      "name": "Constant Language",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#51c5c5",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Constant Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#c792ea",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#98d792"
      }
    },
    {
      "name": "String Escape",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#85c3f4"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#e7d175"
      }
    },
    {
      "name": "Entity Class",
      "scope": ["entity.name.class", "entity.name.type.class"],
      "settings": {
        "foreground": "#e7d175"
      }
    },
    {
      "name": "Entity Class Reference",
