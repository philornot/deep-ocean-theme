{
  "name": "Deep Ocean Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#091014",
    "editor.foreground": "#b1b7be",
    "editor.selectionBackground": "#324753",
    "editor.selectionHighlightBackground": "#1c2a3280",
    "statusBar.background": "#253641",
    "activityBar.background": "#000000",
    "activityBarBadge.background": "#007acc",
    "titleBar.activeBackground": "#091014",
    "tab.activeBackground": "#091014",
    "tab.activeForeground": "#ffffff",
    "tab.inactiveBackground": "#091014",
    "tab.inactiveForeground": "#c6ccd3",
    "editorLineNumber.foreground": "#364555",
    "editorLineNumber.activeForeground": "#607D86",
    "editorCursor.foreground": "#c6ccd3",
    "editorWhitespace.foreground": "#2F4158",
    "terminal.background": "#091014",
    "terminal.foreground": "#c6ccd3",
    "sideBar.background": "#091014",
    "sideBarTitle.foreground": "#bbbbbb",
    "editor.lineHighlightBackground": "#0A1318",
    "editorRuler.foreground": "#1A2D34",
    "editorIndentGuide.background1": "#20323D",
    "editorIndentGuide.activeBackground1": "#3C5066",
    "editorBracketHighlight.foreground1": "#485C7B",
    "gitDecoration.addedResourceForeground": "#98D792",
    "gitDecoration.modifiedResourceForeground": "#FF9E60",
    "gitDecoration.deletedResourceForeground": "#FF5370",
    "diffEditor.insertedTextBackground": "#214F1B80",
    "diffEditor.removedTextBackground": "#602A3080",
    "notificationCenter.border": "#3D1A11",
    "notifications.background": "#3D1A11"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block",
        "comment.line",
        "comment.documentation"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4d6470"
      }
    },
    {
      "name": "Comment Documentation Tags",
      "scope": ["comment.documentation.tag"],
      "settings": {
        "foreground": "#efaeef",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Comment Documentation Values",
      "scope": ["comment.documentation.value"],
      "settings": {
        "foreground": "#cdd3de",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder", "identifier"],
      "settings": {
        "foreground": "#cdd3de"
      }
    },
    {
      "name": "Variable Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#ff9e60"
      }
    },
    {
      "name": "Variable Other",
      "scope": ["variable.other"],
      "settings": {
        "foreground": "#efaeef",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable Instance",
      "scope": ["variable.other.instance"],
      "settings": {
        "foreground": "#889b9f"
      }
    },
    {
      "name": "Variable Static",
      "scope": ["variable.other.static"],
      "settings": {
        "foreground": "#818b8f",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variable Readwrite",
      "scope": ["variable.other.readwrite"],
      "settings": {
        "foreground": "#eeffe3",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Invalid String Escape",
      "scope": ["invalid.illegal.character.escape"],
      "settings": {
        "foreground": "#FF5370",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#C3CEE3",
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#efaeef"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution",
        "keyword.operator"
      ],
      "settings": {
        "foreground": "#51c5c5"
      }
    },
    {
      "name": "Punctuation Brackets",
      "scope": [
        "punctuation.definition.brackets",
        "punctuation.definition.parameters"
      ],
      "settings": {
        "foreground": "#3b8c8c"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f36e77"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#68caff"
      }
    },
    {
      "name": "Function Instance",
      "scope": ["entity.name.function.instance"],
      "settings": {
        "foreground": "#68caff"
      }
    },
    {
      "name": "Function Static",
      "scope": ["entity.name.function.static"],
      "settings": {
        "foreground": "#68caff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#f07178"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
